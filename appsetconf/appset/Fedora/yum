update=yum check-update
upgrade=yum -y update
install=yum -y install
remove=yum -y remove
clean_cache=yum -y clean all
query_upgradable=yum check-update
query_local_byname=yum -C list installed
query_local_info_byname=yum -C info installed
query_remote_byname=yum -C list
query_remote_info_byname=yum -C info
tool_ignore_upgrades=*
tool_hold_upgrades=*
tool_unhold_upgrades=*
check_install_deps=sh /etc/appset/Fedora/yumdeps
check_remove_deps=pacman -Rcp --print-format "%r/%n %s"
check_upgrade_deps=sh /etc/appset/Fedora/yumdeps
download_path=/var/cache/pacman/pkg/
tool_lock_file=/var/lib/pacman/db.lck
install_local=yum install -y
local_ext=.rpm
check_local_deps=sh /etc/appset/Fedora/yumdeps
tool_post_up_cmd=*

query_filter_regexp=^[^ ]+ [^ ]+$
query_repo_sep=/
query_name_regexp=^[[:alpha:]][^\\ ]+
query_lversion_regexp=[^:\\ ]+$
query_info_name_regexp=^Name[[:space:]]+:[ ]+
query_info_version_regexp=^Version[[:space:]]+:[ ]+
query_info_group_regexp=^Groups[[:space:]]+:[ ]+
query_info_license_regexp=^Licenses[[:space:]]+:[ ]+
query_info_description_regexp=^Description[[:space:]]+:[ ]+
query_info_url_regexp=^URL[[:space:]]+:[ ]+
query_info_ksize_regexp=^Download[[:space:]]Size[[:space:]]+:[ ]+
